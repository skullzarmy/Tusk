{
    "name": "tusk-mastodon",
    "description": "Async Mastodon API client for node",
    "homepage": "https://skullzarmy.github.io/Tusk/",
    "version": "2.4.0",
    "author": "skullzarmy",
    "keywords": [
        "mastodon",
        "api",
        "rest",
        "async",
        "axios",
        "tusk"
    ],
    "dependencies": {
        "@jsdoc/salty": "^0.2.9",
        "axios": "^1.10.0"
    },
    "devDependencies": {
        "better-docs": "^2.7.3",
        "eslint": "^9.0.0",
        "jsdoc": "^4.0.0",
        "prettier": "^3.0.0"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "license": "MIT",
    "main": "./lib/mastodon",
    "repository": {
        "type": "git",
        "url": "https://github.com/skullzarmy/Tusk"
    },
    "scripts": {
        "test": "bun test",
        "lint": "eslint lib/ tests/",
        "lint:fix": "eslint lib/ tests/ --fix",
        "format": "prettier --write lib/ tests/ *.json *.md",
        "check-updates": "bun outdated",
        "docs": "jsdoc lib -c jsdoc.json -t node_modules/better-docs -R README.md -d docs"
    }
}
